{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@shared/shared/(.*)": "<rootDir>/../libs/shared/src/$1",
    "@shared/shared": "<rootDir>/../libs/shared/src",
    "@user/user/(.*)": "<rootDir>/../libs/user/src/$1",
    "@user/user": "<rootDir>/../libs/user/src",
    "@people/people/(.*)": "<rootDir>/../libs/people/src/$1",
    "@people/people": "<rootDir>/../libs/people/src",
    "@address/address/(.*)": "<rootDir>/../libs/address/src/$1",
    "@address/address": "<rootDir>/../libs/address/src",
    "@unit_of_measurement/unit-of-measurement/(.*)": "<rootDir>/../libs/unit-of-measurement/src/$1",
    "@unit_of_measurement/unit-of-measurement": "<rootDir>/../libs/unit-of-measurement/src",
    "@category/category/(.*)": "<rootDir>/../libs/category/src/$1",
    "@category/category": "<rootDir>/../libs/category/src",
    "@deposit/deposit/(.*)": "<rootDir>/../libs/deposit/src/$1",
    "@deposit/deposit": "<rootDir>/../libs/deposit/src",
    "@product/product/(.*)": "<rootDir>/../libs/product/src/$1",
    "@product/product": "<rootDir>/../libs/product/src",
    "@inventory/inventory/(.*)": "<rootDir>/../libs/inventory/src/$1",
    "@inventory/inventory": "<rootDir>/../libs/inventory/src"
  }
}