{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "allowJs": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@shared/shared": [
        "modules/shared/src"
      ],
      "@shared/shared/*": [
        "modules/shared/src/*"
      ],
      "@user/user": [
        "modules/user/src"
      ],
      "@user/user/*": [
        "modules/user/src/*"
      ],
      "@people/people": [
        "modules/people/src"
      ],
      "@people/people/*": [
        "modules/people/src/*"
      ],
      "@address/address": [
        "modules/address/src"
      ],
      "@address/address/*": [
        "modules/address/src/*"
      ],
      "@unit_of_measurement/unit-of-measurement": [
        "modules/unit-of-measurement/src"
      ],
      "@unit_of_measurement/unit-of-measurement/*": [
        "modules/unit-of-measurement/src/*"
      ],
      "@category/category": [
        "modules/category/src"
      ],
      "@category/category/*": [
        "modules/category/src/*"
      ],
      "@deposit/deposit": [
        "modules/deposit/src"
      ],
      "@deposit/deposit/*": [
        "modules/deposit/src/*"
      ],
      "@product/product": [
        "modules/product/src"
      ],
      "@product/product/*": [
        "modules/product/src/*"
      ],
      "@inventory/inventory": [
        "modules/inventory/src"
      ],
      "@inventory/inventory/*": [
        "modules/inventory/src/*"
      ]
    }
  }
}